#include "Peri_Adc.h"
#include "adc.h"

void Peri_Adc_Init(uint8_t adc_num)
{
	switch(adc_num)
	{
	case PERI_ADC1:
		MX_ADC1_Init();
		break;

	case PERI_ADC2:

		break;

	case PERI_ADC3:

		break;
	}
}

void Peri_Adc_DeInit(ADC_HandleTypeDef* adcHandle)
{
	switch(adc_num)
	{
	case PERI_ADC1:
		MX_ADC1_Init();
		break;

	case PERI_ADC2:

		break;

	case PERI_ADC3:

		break;
	}
}
